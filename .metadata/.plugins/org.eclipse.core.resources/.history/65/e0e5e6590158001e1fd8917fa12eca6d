package classPractice;

import java.util.Scanner;

public class PrintTriangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		for (char i = 'a'; i <= 'z'; i++) {
			for (char j = 'a'; j <= 'z'; j++) {
				System.out.print(i);
				System.out.print(j);
				System.out.print(" ");
			}
			System.out.println();
		}
		for (char i = 0; i < 6; i++) {
			System.out.print(*);
		}
		scan.close();
	}

}
